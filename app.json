{
    "name": "webrtc-object-detection",
    "version": "1.0.0",
    "description": "Real-time WebRTC Object Detection with dual-mode AI inference",
    "main": "server.js",
    "scripts": {
        "start": "npm run build && npm run start:prod",
        "start:prod": "next start -p $PORT",
        "build": "next build",
        "dev": "next dev -p $PORT"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ananya5151/object-detection-with-YOLO.git"
    },
    "env": {
        "NODE_ENV": "production",
        "MODE": "wasm",
        "NEXT_TELEMETRY_DISABLED": "1"
    }
}